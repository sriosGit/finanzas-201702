<p id="notice"><%= notice %></p>

<h1>Afps</h1>

<table>
  <thead>
    <tr>
      <th>Description</th>
      <th>Percentage</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @afps.each do |afp| %>
      <tr>
        <td><%= afp.description %></td>
        <td><%= afp.rate %></td>
      
        <td><%= link_to 'Show', afp %></td>
        <td><%= link_to 'Edit', edit_afp_path(afp) %></td>
        <td><%= link_to 'Destroy', afp, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Afp', new_afp_path %>
